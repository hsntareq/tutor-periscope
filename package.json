{
    "name": "tutor-periscope",
    "version": "2.0.0",
    "description": "Tutor Periscope",
    "main": "index.js",
    "scripts": {
        "build": "composer install && webpack --env=build --mode=production && _GULP_ENV='build' gulp build",
        "build-dev": "webpack --mode=development && gulp build",
        "css-watch": "gulp watch",
        "js-watch": "webpack --watch --mode=development",
        "watch": "npm run build-dev && concurrently \"npm run css-watch\" \"npm run js-watch\"",
        "lib-watch": "concurrently \"npm run css-watch\" \"npm run js-watch\""
    },
    "keywords": [
        "tutor-periscope",
        "periscope",
        "wordpress"
    ],
    "author": "hasan tareq",
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^6.3.0",
        "css-loader": "^6.3.0",
        "css-minimizer-webpack-plugin": "^3.1.0",
        "eslint": "^7.32.0",
        "eslint-config-wordpress": "^2.0.0",
        "eslint-loader": "^4.0.2",
        "fs": "0.0.1-security",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-clean": "^0.4.0",
        "gulp-cssnano": "^2.1.3",
        "gulp-notify": "^4.0.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-wp-pot": "^2.5.0",
        "gulp-zip": "^5.1.0",
        "node-sass": "^6.0.1",
        "path": "^0.12.7",
        "po2json": "^0.4.5",
        "sass-loader": "^12.1.0",
        "uglify-js": "^3.14.2",
        "webpack": "^5.57.0",
        "webpack-cli": "^4.8.0"
    },
    "dependencies": {
        "@wordpress/media-utils": "^3.0.1",
        "html2pdf.js": "^0.10.1",
        "mini-css-extract-plugin": "^2.3.0",
        "webpack-dev-server": "^4.3.1"
    }
}
